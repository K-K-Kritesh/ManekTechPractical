{
  "name": "maneltechpractical",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "compile": "rm -rf dist && tsc",
    "android:assembleRelease": "cd android && ./gradlew assembleRelease",
    "android:assembleDebug": "cd android && ./gradlew assembleDebug",
    "android:clean": "cd android && ./gradlew clean",
    "android:dev": "concurrently \"tsc -w\" \"npm run android:clean && react-native run-android\"",
    "android:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && npm run compile && npm run android:clean && npm run android:assembleRelease",
    "android:debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && npm run compile && npm run android:clean && npm run android:assembleDebug",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "axios": "^0.27.2",
    "axios-hooks": "^3.0.4",
    "concurrently": "^7.2.1",
    "formik": "^2.2.9",
    "lru-cache": "^7.10.1",
    "ramda": "^0.28.0",
    "react": "17.0.2",
    "react-if": "^4.1.4",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-get-location": "^2.2.1",
    "react-native-maps": "^0.31.1",
    "react-native-maps-directions": "^1.8.0",
    "react-native-permissions": "^3.3.1",
    "react-native-ratings": "^8.1.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lru-cache": "^7.10.10",
    "@types/ramda": "^0.28.13",
    "@types/react-native": "^0.67.3",
    "@types/react-native-sqlite-storage": "^5.0.2",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
